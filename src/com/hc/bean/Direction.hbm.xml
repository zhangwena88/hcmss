<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="com.hc.bean.Direction" table="direction">
		<id name="direction_id" column="direction_id">
			<generator class="native"/>
		</id>
		
		<property name="direction_name" column="direction_name"/>
	</class>
	
</hibernate-mapping>    